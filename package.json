{
  "name": "tennistab",
  "version": "1.0.0",
  "description": "테니스 협회 클럽 및 회원 관리 시스템",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "echo 'ESLint is configured for lint-staged. Run: npm run lint:check'",
    "lint:check": "eslint src --max-warnings 0 || true",
    "lint:fix": "eslint src --fix",
    "stylelint:check": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git@github-personal:jeonghakhur/tennistab.git"
  },
  "keywords": [
    "tennis",
    "association",
    "club",
    "tournament"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@restart/hooks": "^0.6.2",
    "@restart/ui": "^2.0.0-beta.10",
    "@sanity/client": "^7.13.0",
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.16.0",
    "@types/react-transition-group": "^4.4.12",
    "bootstrap": "^5.3.8",
    "clsx": "^2.1.1",
    "dom-helpers": "^6.0.1",
    "invariant": "^2.2.4",
    "next": "^16.0.3",
    "next-sanity": "^11.6.8",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-transition-group": "^4.4.5",
    "sanity": "^4.16.0",
    "styled-components": "^6.1.19",
    "uncontrollable": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@sanity/types": "^4.16.0",
    "@types/invariant": "^2.2.37",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "stylelint": "^16.26.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
